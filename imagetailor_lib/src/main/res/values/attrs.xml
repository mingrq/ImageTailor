<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CropImageView">
        <!--裁剪类型-->
        <attr name="cropType" format="enum">
            <enum name="rectangle" value="0x45851185" /> <!--矩形-->
            <enum name="oval" value="0x48424813" /><!--椭圆-->
        </attr>
        <attr name="cropWidth" format="dimension" /><!--裁剪宽度-->
        <attr name="cropHeight" format="dimension" /><!--裁剪高度-->

    </declare-styleable>
</resources>